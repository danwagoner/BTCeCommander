#region Variables
$global:refresh = 2000	; # number of seconds to wait between refreshing data
$count = 14; # number of orders to return
$rounddec = 8; # number of decimals to round
$global:wall = 300; # any quantity above this number is considered a wall
$wallcolor = "LightGray"

$global:uri = "https://wex.nz"
$global:apiuri = $uri + "/tapi"
$tradepairs = @("btc_usd", "btc_rur", "btc_eur", "ltc_btc", "ltc_usd", "ltc_rur", "nmc_btc", "usd_rur", "eur_usd", "nvc_btc", "trc_btc", "ppc_btc", "ftc_btc")

$progressPreference = 'silentlyContinue'
#endregion

#region Functions
function PopUp([string] $message){
	[System.Reflection.Assembly]::LoadWithPartialName("System.Windows.Forms")
	[System.Windows.Forms.MessageBox]::Show($message)
}

function InputBox([string] $title, [string] $message, [string] $defaultText){
	[void] [System.Reflection.Assembly]::LoadWithPartialName("System.Drawing")
	[void] [System.Reflection.Assembly]::LoadWithPartialName("System.Windows.Forms")
	$userForm = New-Object System.Windows.Forms.Form
	$userForm.Text = "$title"
	$userForm.Size = New-Object System.Drawing.Size(380,260)
	$userForm.StartPosition = "CenterScreen"
    $userForm.AutoSize = $false
    $userForm.MinimizeBox = $False
    $userForm.MaximizeBox = $False
    $userForm.SizeGripStyle= "Hide"
    $userForm.WindowState = "Normal"
    $userForm.FormBorderStyle="Fixed3D"
   
	$OKButton = New-Object System.Windows.Forms.Button
	$OKButton.Location = New-Object System.Drawing.Size(135,180)
	$OKButton.Size = New-Object System.Drawing.Size(50,23)
	$OKButton.Text = "OK"
	$OKButton.Add_Click({$value=$objTextBox.Text;$userForm.Close()})
	$userForm.Controls.Add($OKButton)
	$CancelButton = New-Object System.Windows.Forms.Button
	$CancelButton.Location = New-Object System.Drawing.Size(185,180)
	$CancelButton.Size = New-Object System.Drawing.Size(50,23)
	$CancelButton.Text = "Cancel"
	$CancelButton.Add_Click({$userForm.Close()})
	$userForm.Controls.Add($CancelButton)
	$userLabel = New-Object System.Windows.Forms.Label
	$userLabel.Location = New-Object System.Drawing.Size(10,20)
	$userLabel.Size = New-Object System.Drawing.Size(350,100)
	$userLabel.Text = "$message"
	$userForm.Controls.Add($userLabel)
	$objTextBox = New-Object System.Windows.Forms.TextBox
	$objTextBox.Location = New-Object System.Drawing.Size(135,150)
	$objTextBox.Size = New-Object System.Drawing.Size(100,20)
	$objTextBox.Text="$defaultText"
	$objTextBox.AcceptsReturn=$true
	$userForm.Controls.Add($objTextBox)
	$userForm.Topmost = $True
	$userForm.Opacity = 0.91
	$userForm.ShowIcon = $False
	$userForm.Add_Shown({$userForm.Activate()})
	[void] $userForm.ShowDialog()
	return $objTextBox.Text
}

function UpdateBTCeOrderBook{
	
	$result = (Invoke-WebRequest -Uri $depthuri -TimeoutSec 15).Content| ConvertFrom-Json
	$ask = @()
	$bid = @()
	
	# Build ask array
	if($result.error -eq $null){
		Set-Variable -Name sellhigh -Value $result.asks[0][0] -Scope Global
		for($x=0;$x -lt $count;$x++){
			$asktotal += $result.asks[$x][1]
			$Builder = New-Object System.Object
			$Builder | Add-Member -Type NoteProperty -Name "Price" -Value ([math]::round($result.asks[$x][0], $rounddec))
			$Builder | Add-Member -Type NoteProperty -Name "Quantity" -Value ([math]::round($result.asks[$x][1], $rounddec))
			$Builder | Add-Member -Type NoteProperty -Name "Total" -Value ([math]::round(($result.asks[$x][0])*($result.asks[$x][1]),2))
			$ask += $Builder
		}
		
		# Build bid array
		Set-Variable -Name bidhigh -Value $result.bids[0][0] -Scope Global
		for($x=0;$x -lt $count;$x++){
			$bidtotal += $result.bids[$x][1]
			$Builder = New-Object System.Object
			$Builder | Add-Member -Type NoteProperty -Name "Price" -Value ([math]::round($result.bids[$x][0], $rounddec))
			$Builder | Add-Member -Type NoteProperty -Name "Quantity" -Value ([math]::round($result.bids[$x][1], $rounddec))
			$Builder | Add-Member -Type NoteProperty -Name "Total" -Value ([math]::round(($result.bids[$x][0])*($result.bids[$x][1]),2))
			$bid += $Builder
		}
		
		Load-DataGridView -DataGridView $dgv_ask -Item $ask
		Load-DataGridView -DataGridView $dgv_bid -Item $bid
		
		# Ask wall visual changes
		for($x=0;$x -le $dgv_ask.RowCount-1;$x++){
			$currentcellval = $dgv_ask.Item(1,$x).Value
			if ($currentcellval -ge $wall){
				$dgv_ask.Item(0,$x).Style.BackColor = $wallcolor
				$dgv_ask.Item(1,$x).Style.BackColor = $wallcolor
				$dgv_ask.Item(2,$x).Style.BackColor = $wallcolor
			}
		}
		
		# Buy wall visual changes
		for($x=0;$x -le $dgv_bid.RowCount-1;$x++){
			$currentcellval = $dgv_bid.Item(1,$x).Value
			if ($currentcellval -ge $wall){
				$dgv_bid.Item(0,$x).Style.BackColor = $wallcolor
				$dgv_bid.Item(1,$x).Style.BackColor = $wallcolor
				$dgv_bid.Item(2,$x).Style.BackColor = $wallcolor
			}
		}
		
		$lblsell_volume.Text = "Sell Total: $([math]::round($asktotal,0))"
		$lblbuy_volume.Text = "Buy Total: $([math]::round($bidtotal,0))"
	}
	else{PopUp("Error: $($result.error)")}
}

function GetBTCeFee($x){
	$feeuri = "$($uri)/api/2/$x/fee"
	$feeresult = (Invoke-WebRequest -Uri $feeuri).Content | ConvertFrom-Json
	return $feeresult.trade
}

function GetStringHash([String] $String) { 
	
	$hmacsha = New-Object System.Security.Cryptography.HMACSHA512
	$hmacsha.key = [Text.Encoding]::ASCII.GetBytes($apisecret)
	$signature = $hmacsha.ComputeHash([Text.Encoding]::ASCII.GetBytes($String))
	$sig = [string]::join("", ($signature | % {([int]$_).toString('x2')}))
	return $sig
}

function GetUserInfo{
	$nonce = (Get-Date -uformat %s) -Replace("[,\.]\d*", "")
	$postdata = "method=getInfo&nonce=$nonce"
	$signedpost = GetStringHash($postdata)
	$r = Invoke-WebRequest -Uri $apiuri -Method POST -Headers @{"Key" = $apikey; "Sign" = $signedpost} -Body $postdata -ContentType "application/x-www-form-urlencoded" -UserAgent "Mozilla/5.0 (Windows; U; Windows NT 6.1; ru; rv:1.9.2.3) Gecko/20100401 Firefox/4.0 (.NET CLR 3.5.30729)"
	##write-host $r
	$response = ConvertFrom-Json $r
	if($response.success -eq "1"){
		$funds = @()
		$Builder = New-Object System.Object
		$Builder | Add-Member -Type NoteProperty -Name "USD" -Value $response.return.funds.usd
		$Builder | Add-Member -Type NoteProperty -Name "BTC" -Value $response.return.funds.btc
		$Builder | Add-Member -Type NoteProperty -Name "LTC" -Value $response.return.funds.ltc
		$Builder | Add-Member -Type NoteProperty -Name "NMC" -Value $response.return.funds.nmc
		$Builder | Add-Member -Type NoteProperty -Name "RUR" -Value $response.return.funds.rur
		$Builder | Add-Member -Type NoteProperty -Name "EUR" -Value $response.return.funds.eur
		$Builder | Add-Member -Type NoteProperty -Name "NVC" -Value $response.return.funds.nvc
		$Builder | Add-Member -Type NoteProperty -Name "TRC" -Value $response.return.funds.trc
		$Builder | Add-Member -Type NoteProperty -Name "PPC" -Value $response.return.funds.ppc
		$Builder | Add-Member -Type NoteProperty -Name "FTC" -Value $response.return.funds.ftc
		$Builder | Add-Member -Type NoteProperty -Name "XPM" -Value $response.return.funds.xpm
		$funds += $Builder
		
		Load-DataGridView -DataGridView $dgvfunds -Item $funds
		$global:availsellfunds = $funds.$pairfirst
		$txt_sellavail.Text = $availsellfunds
		$global:availbuyfunds = $funds.$pairsecond
		$txt_buyavail.Text = $availbuyfunds
	}
	else{ PopUp("Information could not be retrieved from BTC-e with the given API key/secret. Account information will not be available until the issue is resolved.")}
}

function GetOrderInfo{
	$nonce = (Get-Date -uformat %s) -Replace("[,\.]\d*", "")
	$postdata = "method=OrderList&active=0&nonce=$nonce"
	$signedpost = GetStringHash($postdata)
	$r = Invoke-WebRequest -Uri $apiuri -Method POST -Headers @{"Key" = $apikey; "Sign" = $signedpost} -Body $postdata -ContentType "application/x-www-form-urlencoded" -UserAgent "Mozilla/5.0 (Windows; U; Windows NT 6.1; ru; rv:1.9.2.3) Gecko/20100401 Firefox/4.0 (.NET CLR 3.5.30729)"
	#write-host $r
	$response = ConvertFrom-Json $r
	
	if ($response.success -eq 1){
		$dgv_orders.Visible = $true
		$ordernumber += $response.return | gm | ?{$_.Name -match "^\d"} | Select Name
		$orderhistory = @()
	
		foreach($order in $ordernumber){
			$Builder = New-Object System.Object
			$Builder | Add-Member -Type NoteProperty -Name "Order" -Value $order.name
			$Builder | Add-Member -Type NoteProperty -Name "Pair" -Value $response.return.$($order.name).pair
			$Builder | Add-Member -Type NoteProperty -Name "Type" -Value $response.return.$($order.name).type
			$Builder | Add-Member -Type NoteProperty -Name "Amount" -Value $response.return.$($order.name).amount
			$Builder | Add-Member -Type NoteProperty -Name "Rate" -Value $response.return.$($order.name).rate
			$orderhistory += $Builder
			Load-DataGridView -DataGridView $dgv_orders -Item $orderhistory
		}
	} 
	elseif(($response.success -eq 0) -and ($response.error -eq "no orders")){
		$dgv_orders.Visible = $false
	}
	else{PopUp("There was an issue accessing your order information. Error message from BTCe:`r`n`r`n$($response.error)")}
}

function GetTradeHistory{
