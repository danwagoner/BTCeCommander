	$r = Invoke-WebRequest -Uri $apiuri -Method POST -Headers @{"Key" = $apikey; "Sign" = $signedpost} -Body $postdata -ContentType "application/x-www-form-urlencoded" -UserAgent "Mozilla/5.0 (Windows; U; Windows NT 6.1; ru; rv:1.9.2.3) Gecko/20100401 Firefox/4.0 (.NET CLR 3.5.30729)"
	#write-host $r
	$response = ConvertFrom-Json $r
	
	if ($response.success -eq 1){
		#$dgv_trades.Visible = $true
		$tradenumber += $response.return | gm | ?{$_.Name -match "^\d"} | Select Name
		$tradehistory = @()
	
		foreach($trade in $tradenumber){
		$Builder = New-Object System.Object
			$Builder | Add-Member -Type NoteProperty -Name "Trade" -Value $trade.name
			$Builder | Add-Member -Type NoteProperty -Name "OrderID" -Value $response.return.$($trade.name).order_id
			$Builder | Add-Member -Type NoteProperty -Name "Pair" -Value $response.return.$($trade.name).pair
			$Builder | Add-Member -Type NoteProperty -Name "Type" -Value $response.return.$($trade.name).type
			$Builder | Add-Member -Type NoteProperty -Name "Amount" -Value $response.return.$($trade.name).amount
			$Builder | Add-Member -Type NoteProperty -Name "Rate" -Value $response.return.$($trade.name).rate
			$Builder | Add-Member -Type NoteProperty -Name "Is Your Order" -Value $response.return.$($trade.name).is_your_order
			$tradehistory += $Builder
			
			##write-host $tradehistory
			Load-DataGridView -DataGridView $dgv_trades -Item $tradehistory
		}
	}
}

