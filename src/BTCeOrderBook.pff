function ExecuteOrder([string]$tradepair, [string]$tradetype, [string]$traderate, [string]$tradeamount)  {
	$nonce = (Get-Date -uformat %s) -Replace("[,\.]\d*", "")
	$postdata = "method=Trade&nonce=$nonce&pair=$tradepair&type=$tradetype&rate=$traderate&amount=$tradeamount"
	$signedpost = GetStringHash($postdata)
	$r = Invoke-WebRequest -Uri $apiuri -Method POST -Headers @{"Key" = $apikey; "Sign" = $signedpost} -Body $postdata -ContentType "application/x-www-form-urlencoded" -UserAgent "Mozilla/5.0 (Windows; U; Windows NT 6.1; ru; rv:1.9.2.3) Gecko/20100401 Firefox/4.0 (.NET CLR 3.5.30729)"
	#write-host $r
	$response = $r | ConvertFrom-Json
	
	if ($response.success -eq 1){
		PopUp ("Order submitted successfully!")
		Start-Sleep 1
		GetOrderInfo
		Start-Sleep 1
		GetUserInfo
	}else{
		PopUp ("There was an error submitting your order. Error message from BTCe:`r`n`r`n$($response.error)")
	}
}

function CancelOrder([string]$orderid){
	$nonce = (Get-Date -uformat %s) -Replace("[,\.]\d*", "")
	$postdata = "method=CancelOrder&nonce=$nonce&order_id=$orderid"
	$signedpost = GetStringHash($postdata)
	$r = Invoke-WebRequest -Uri $apiuri -Method POST -Headers @{"Key" = $apikey; "Sign" = $signedpost} -Body $postdata -ContentType "application/x-www-form-urlencoded" -UserAgent "Mozilla/5.0 (Windows; U; Windows NT 6.1; ru; rv:1.9.2.3) Gecko/20100401 Firefox/4.0 (.NET CLR 3.5.30729)"
	#write-hostd $r
	$response = $r | ConvertFrom-Json
	
	if ($response.success -eq 1){
		PopUp ("Order cancelled successfully!")
		Start-Sleep 1
		GetOrderInfo
		Start-Sleep 1
		GetUserInfo	
	}else{
		PopUp ("There was an error submitting your order. Error from BTCe:`r`n`r`n$($response.error)")
	}
}

function UpdatePairInfo($p){
	# Set necessary global variables
	Set-Variable -Name pair -Value $p -Scope Global	
	Set-Variable -Name pairid -Value $p.Replace("/", "_").ToLower() -Scope Global
	Set-Variable -Name pairfirst -Value $pair.Split("/")[0].ToUpper() -Scope Global
	Set-Variable -Name pairsecond -Value $pair.Split("/")[1].ToUpper() -Scope Global
	Set-Variable -Name depthuri -Value "$($uri)/api/2/$pairid/depth" -Scope Global
	GetUserInfo
	
	# Modify necessary form controls
	$formMain.Text = "BTC-e Cockpit - $pair"
	$lbl_sellavail.Text = $pairfirst+":"
	$lbl_selltotal.Text = $pairsecond+":"
	$lbl_buyavail.Text = $pairsecond+":"
	$lbl_buytotal.Text = $pairfirst+":"
	$gbox_buy.Text = "Buy "+$pairfirst
	$gbox_sell.Text = "Sell "+$pairfirst
	$txt_buyfee.Clear()
	$txt_buyprice.Clear()
	$txt_buytotal.Clear()
	$txt_sellfee.Clear()
	$txt_sellprice.Clear()
	$txt_selltotal.Clear()
	
}

function CalculateSell{
	[decimal]$calcfeeperc = GetBTCeFee($pairid)
	[decimal]$calcsellamnt = $txt_sellavail.Text
	[decimal]$calcsellprice = $txt_sellprice.Text
	
	if (($calcsellamnt -gt 0) -and ($calcsellprice -gt 0)){
		$calcselltotal = [math]::Round(($calcsellamnt * $calcsellprice), $rounddec)
		[decimal]$calcfee = [math]::round((($calcselltotal * $calcfeeperc) / 100), 4)
		$txt_sellfee.Text = "$calcfee $pairsecond"
		$calcselltotal = $calcselltotal - $calcfee
		$txt_selltotal.Text = [math]::Round($calcselltotal, $rounddec)
	}	
}

function CalculateBuy{
	[decimal]$calcfeeperc = GetBTCeFee($pairid)
	[decimal]$calcbuyavail = $txt_buyavail.Text
	[decimal]$calcbuyprice = $txt_buyprice.Text
	
	if (($calcbuyavail -gt 0) -and ($calcbuyprice -gt 0)){
		$calcbuytotal = [math]::round(($calcbuyavail / $calcbuyprice), $rounddec)
		if ($calcbuytotal -ge 0){
			[decimal]$calcfee = [math]::round((($calcbuytotal * $calcfeeperc) / 100), 4)
			$txt_buyfee.Text = "$calcfee $pairfirst"
			$calcbuytotal = $calcbuytotal - $calcfee #- 0.0001 # <--horrible hack (see todo rounding down)
			$txt_buytotal.Text = [math]::round($calcbuytotal, $rounddec)
		}	
		else {$txt_buytotal.Text = "0"}
	}	
}

